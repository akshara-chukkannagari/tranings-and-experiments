{
  "private": true,
  "engines": {
    "node": ">=6.5.0"
  },
  "scripts": {
    "compile": "java -jar node_modules/google-closure-compiler/compiler.jar -O ADVANCED --language_in=ES6_STRICT --new_type_inf --js_output_file='dist/bundle.js' 'src/**.js' -W VERBOSE",
    "prepublish": "npm run compile",
    "prestart": "npm install",
    "start": "http-server app/ -a 0.0.0.0 -p 8000",
    "test": "grunt test"
  },
  "dependencies": {
    "todomvc-app-css": "^2.0.1",
    "todomvc-common": "^1.0.2",
    "http-server": "^0.9.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "google-closure-compiler": "^20161024.3.0",
    "graceful-fs": "^4.0.0",
    "grunt": "^0.4.5",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^5.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "load-grunt-tasks": "^3.5.2",
    "minimatch": "^3.0.2"
  }
}
